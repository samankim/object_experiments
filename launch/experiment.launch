<launch>
	<!-- TODO: use kinect-demo launch file. Start up the Kinects.
		 Specify the frame rate as an arg. -->
	<include file = "$(find pkg-name)/path/filename.xml">

	<!-- Start up UR10 -->
	<include file = "$(find pkg-name)/launch/filename.xml">

	<!-- Begin robot arm script -->	
	<node
		name = "robot_script" 
		pkg = "moveit_tutorials"
		type = "move_group_python_interface_tutorial.py"	
	/>

	<!-- TODO: Move to another launch file///Record topics in rosbag -->
